{
    "name": "erut-web",
    "version": "14.3.0",
    "description": "erupt framework front-end",
    "author": "yuepeng <erupts@126.com>",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/erupts/erupt-web.git"
    },
    "bugs": {
        "url": "https://github.com/erupts/erupt-web/issues"
    },
    "homepage": "https://www.erupt.xyz",
    "license": "Apache License 2.0",
    "keywords": [
        "erupt",
        "yuepeng",
        "antd",
        "ng-zorro-antd",
        "alain",
        "angular",
        "component"
    ],
    "scripts": {
        "ng-high-memory": "node --max_old_space_size=8000 ./node_modules/@angular/cli/bin/ng",
        "ng": "ng",
        "start": "ng s -o",
        "build": "npm run ng-high-memory build",
        "analyze": "npm run ng-high-memory build -- --source-map",
        "analyze:view": "source-map-explorer dist/**/*.js",
        "color-less": "ng-alain-plugin-theme -t=colorLess",
        "theme": "ng-alain-plugin-theme -t=themeCss",
        "icon": "ng g ng-alain:plugin icon"
    },
    "dependencies": {
        "@angular/animations": "^14.2.0",
        "@angular/common": "^14.2.0",
        "@angular/compiler": "^14.2.0",
        "@angular/core": "^14.2.0",
        "@angular/forms": "^14.2.0",
        "@angular/platform-browser": "^14.2.0",
        "@angular/platform-browser-dynamic": "^14.2.0",
        "@angular/router": "^14.2.0",
        "@antv/g2plot": "^2.4.23",
        "@delon/abc": "^14.3.0",
        "@delon/auth": "^14.3.0",
        "@delon/cache": "^14.3.0",
        "@delon/chart": "^14.3.0",
        "@delon/form": "^14.3.0",
        "@delon/theme": "^14.3.0",
        "@delon/util": "^14.3.0",
        "font-awesome": "^4.7.0",
        "intl": "^1.2.5",
        "moment": "^2.29.4",
        "monaco-editor": "^0.34.1",
        "ng-zorro-antd": "^14.3.0",
        "ngx-tinymce": "^14.0.0",
        "ngx-ueditor": "^14.0.0",
        "qrious": "^4.0.2",
        "rxjs": "~7.5.0",
        "screenfull": "^6.0.2",
        "ts-md5": "^1.3.1",
        "tslib": "^2.3.0",
        "zone.js": "~0.11.4"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "^14.2.1",
        "@angular/cli": "~14.2.1",
        "@angular/compiler-cli": "^14.2.0",
        "@angular/language-service": "^14.2.0",
        "@types/node": "^12.11.1",
        "ng-alain": "^14.3.0",
        "ng-alain-plugin-theme": "^14.0.0",
        "node-fetch": "^2.6.1",
        "source-map-explorer": "^2.5.2",
        "ts-node": "~10.9.1",
        "typescript": "~4.7.4"
    },
    "lint-staged": {
        "(src)/**/*.{html,ts}": [
            "eslint --fix"
        ],
        "(src)/**/*.less": [
            "npm run lint:style"
        ]
    }
}
